{
    "trailingComma": "all",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "plugins": ["prettier-plugin-tailwindcss"],
    "printWidth": 80,                   
    "arrowParens": "always",           
    "bracketSpacing": true,             
    "jsxBracketSameLine": false,        
     "endOfLine": "auto",               
    "proseWrap": "preserve",            
    "htmlWhitespaceSensitivity": "css", 
    "quoteProps": "as-needed",          
    "vueIndentScriptAndStyle": true
}